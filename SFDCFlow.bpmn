      <scriptTask id="calculateResults" name="Calculate Resutls" scriptFormat="javascript" activiti:autoStoreVariables="true">
        <script>// ***** reset party1_email to null;
party1_email = null;
 addedInvitee = false;

// ****** set crrent user map
userLevelCostMap=Packages.java.util.HashMap();
userLevelCostMap.put("ContactManager",   5.00);
userLevelCostMap.put("Group",            25.00);
userLevelCostMap.put("Professional",     65.00);
userLevelCostMap.put("Enterprise",      125.00);
userLevelCostMap.put("Performance",     300.00);
userLevelCostMap.put("NotSure",        125.00);

inputUserLevel = "NotSure";
if (UserLevel != "") { inputUserLevel = UserLevel}
costPerUser = 0 + userLevelCostMap.get(inputUserLevel);

userProjected         =   UserCurrent * 1   +  UserNew * 1;
currentMonthlySpend   =   UserCurrent    *  costPerUser;
projectedMonthlySpend =   userProjected * costPerUser;

 // ****** set crrent user map
currentUserMap=Packages.java.util.HashMap();
currentUserMap.put("dedicated",   UserCurrent * PercentDedicatedUser);
currentUserMap.put("occasional",  UserCurrent * PercentOccasionalUser);
currentUserMap.put("infrequent",  UserCurrent * PercentInfrequentUser);
currentUserMap.put("thirdParty",  UserCurrent * Percent3rdPartyUser);

 // ****** set projected user map
projectedUserMap=Packages.java.util.HashMap();
projectedUserMap.put("dedicated",   userProjected * PercentDedicatedUser);
projectedUserMap.put("occasional",  userProjected * PercentOccasionalUser);
projectedUserMap.put("infrequent",  userProjected * PercentInfrequentUser);
projectedUserMap.put("thirdParty",  userProjected * Percent3rdPartyUser);

 // ****** set min save Assumption map
minSaveAssumptionMap=Packages.java.util.HashMap();
minSaveAssumptionMap.put("dedicated",   "1");
minSaveAssumptionMap.put("occasional",  "0.75");
minSaveAssumptionMap.put("infrequent",  "0.5");
minSaveAssumptionMap.put("thirdParty",  "0.25");

 // ****** set min current user map
minUserCurrentPIOMap=Packages.java.util.HashMap();
minUserCurrentPIOMap.put("dedicated",   currentUserMap.get("dedicated")  * minSaveAssumptionMap.get("dedicated"));
minUserCurrentPIOMap.put("occasional",  currentUserMap.get("occasional") * minSaveAssumptionMap.get("occasional"));
minUserCurrentPIOMap.put("infrequent",  currentUserMap.get("infrequent") * minSaveAssumptionMap.get("infrequent"));
minUserCurrentPIOMap.put("thirdParty",  currentUserMap.get("thirdParty") * minSaveAssumptionMap.get("thirdParty"));

lowEndWithPIOCurrentUser = minUserCurrentPIOMap.get("dedicated") * 1
						+  minUserCurrentPIOMap.get("occasional") * 1
						+  minUserCurrentPIOMap.get("infrequent") * 1
						+  minUserCurrentPIOMap.get("thirdParty") * 1;

lowEndWithPIOCurrentCost = lowEndWithPIOCurrentUser * costPerUser;

 // ****** set min projected user map
minUserProjectedPIOMap=Packages.java.util.HashMap();
minUserProjectedPIOMap.put("dedicated",   projectedUserMap.get("dedicated")  * minSaveAssumptionMap.get("dedicated"));
minUserProjectedPIOMap.put("occasional",  projectedUserMap.get("occasional") * minSaveAssumptionMap.get("occasional"));
minUserProjectedPIOMap.put("infrequent",  projectedUserMap.get("infrequent") * minSaveAssumptionMap.get("infrequent"));
minUserProjectedPIOMap.put("thirdParty",  projectedUserMap.get("thirdParty") * minSaveAssumptionMap.get("thirdParty"));

lowEndWithPIOProjectedUser = minUserProjectedPIOMap.get("dedicated") * 1
						+  minUserProjectedPIOMap.get("occasional") * 1
						+  minUserProjectedPIOMap.get("infrequent") * 1
						+  minUserProjectedPIOMap.get("thirdParty")* 1;

lowEndWithPIOProjectedCost = lowEndWithPIOProjectedUser * costPerUser;


 // ****** set max save Assumption map
maxSaveAssumptionMap=Packages.java.util.HashMap();
maxSaveAssumptionMap.put("dedicated",   "0.75");
maxSaveAssumptionMap.put("occasional",  "0.5");
maxSaveAssumptionMap.put("infrequent",  "0.0");
maxSaveAssumptionMap.put("thirdParty",  "0.0");

 // ****** set max projected user map
maxUserProjectedPIOMap=Packages.java.util.HashMap();
maxUserProjectedPIOMap.put("dedicated",   projectedUserMap.get("dedicated")  * maxSaveAssumptionMap.get("dedicated"));
maxUserProjectedPIOMap.put("occasional",  projectedUserMap.get("occasional") * maxSaveAssumptionMap.get("occasional"));
maxUserProjectedPIOMap.put("infrequent",  projectedUserMap.get("infrequent") * maxSaveAssumptionMap.get("infrequent"));
maxUserProjectedPIOMap.put("thirdParty",  projectedUserMap.get("thirdParty") * maxSaveAssumptionMap.get("thirdParty"));

highEndWithPIOProjectedUser = maxUserProjectedPIOMap.get("dedicated")* 1 
						+  maxUserProjectedPIOMap.get("occasional") * 1
						+  maxUserProjectedPIOMap.get("infrequent") * 1
						+  maxUserProjectedPIOMap.get("thirdParty")* 1;

highEndWithPIOProjectedCost = highEndWithPIOProjectedUser * costPerUser;

 // ***** for display in Task 3
 CostPercentagePIO = 0.5;

lowPotentialMonthlySavings   = currentMonthlySpend - lowEndWithPIOCurrentCost;
highPotentialMonthlySavings = lowEndWithPIOProjectedCost * 1 - highEndWithPIOProjectedCost * 1;
lowPIOCost   = lowPotentialMonthlySavings   * CostPercentagePIO;
highPIOCost = highPotentialMonthlySavings * CostPercentagePIO;

lowProjectedYearlySaving  = lowPotentialMonthlySavings  * (1-CostPercentagePIO* 1) * 12;
highProjectedYearlySaving = highPotentialMonthlySavings * (1-CostPercentagePIO* 1) * 12;

lowPotentialSavingPercent = lowPotentialMonthlySavings / currentMonthlySpend * 100.00;
highPotentialSavingPercent = highPotentialMonthlySavings / projectedMonthlySpend * 100.00;

showYearlySavings = " $" + lowProjectedYearlySaving.toFixed(2) + " ~ $" + highProjectedYearlySaving.toFixed(2);
showSavingsPercentage = " " + lowPotentialSavingPercent.toFixed(2) + "% ~ " + highPotentialSavingPercent.toFixed(2) + "%";

//***** set for display
displayPercentDedicatedUser = PercentDedicatedUser * 100;
displayPercentOccasionalUser = PercentOccasionalUser * 100;
displayPercentInfrequentUser = PercentInfrequentUser * 100;
displayPercent3rdPartyUser = Percent3rdPartyUser * 100;
displaylowPotentialSavingPercent = lowPotentialSavingPercent * 100;
displayhighPotentialSavingPercent = highPotentialSavingPercent * 100;
displayhighProjectedYearlySaving = "$" + highProjectedYearlySaving.toFixed(2);
</script>
      </scriptTask>
